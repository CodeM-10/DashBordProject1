                                                td 12345678
                                                td: i.lni.lni-wallet.type_icon
                                                td Bitcnin
                                                td $100
                                                td #18000td 
                                                td.Status: p completed
                                                td: i.material-icons.round_i more_vert
                                .more View More

    include layout/scripts.pug 
